{
  "name": "clits",
  "version": "0.3.0",
  "description": "CLI tool and library for extracting and sharing debugging data for AI and web projects.",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./cli": {
      "import": "./dist/cli.js"
    }
  },
  "bin": {
    "clits": "dist/cli.js",
    "clits-inspect": "dist/cli-inspect.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "start": "node dist/cli.js",
    "dev": "ts-node src/cli.ts",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test && npm run lint",
    "test": "vitest run --exclude 'src/**/*.spec.ts'",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "vitest run 'src/**/*.spec.ts'"
  },
  "keywords": [
    "cli",
    "debugging",
    "ai",
    "logs",
    "extractor",
    "chrome",
    "devtools",
    "debug",
    "logging",
    "error-handling",
    "monitoring",
    "diagnostics",
    "troubleshooting",
    "log-analysis",
    "chrome-devtools-protocol",
    "development-tools",
    "testing"
  ],
  "author": "Jason Vaughan",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jasonvaughan/ai-debug-extractor.git"
  },
  "bugs": {
    "url": "https://github.com/jasonvaughan/ai-debug-extractor/issues"
  },
  "engines": {
    "node": ">=16"
  },
  "devDependencies": {
    "@playwright/test": "^1.52.0",
    "@types/inquirer": "^9.0.8",
    "@types/node": "^20.11.0",
    "@types/node-fetch": "^2.6.12",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "@vitest/coverage-c8": "^0.33.0",
    "@vitest/ui": "^3.2.1",
    "eslint": "^8.57.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vitest": "^3.2.1"
  },
  "dependencies": {
    "chalk": "^5.4.1",
    "chrome-remote-interface": "^0.33.3",
    "commander": "^12.0.0",
    "fast-glob": "^3.3.3",
    "glob": "^11.0.2",
    "inquirer": "^9.3.7",
    "node-fetch": "^3.3.2",
    "ora": "^8.2.0",
    "winston": "^3.17.0"
  }
}

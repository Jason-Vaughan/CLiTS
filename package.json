{
  "name": "@puberty-labs/clits",
  "version": "1.0.4-beta.0",
  "description": "Chrome Log Inspector & Troubleshooting System (CLiTS) - A tool for extracting and sharing debugging data for AI and web projects.",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "logo": "assets/CLiTS Logo.png",
  "homepage": "https://github.com/Jason-Vaughan/CLiTS#readme",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./cli": {
      "import": "./dist/cli.js"
    }
  },
  "bin": {
    "clits": "dist/cli.js",
    "clits-inspect": "dist/cli-inspect.js"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "!dist/**/*.test.js",
    "!dist/**/__tests__/**",
    "README.md",
    "LICENSE",
    "assets/**/*.png"
  ],
  "scripts": {
    "build": "tsc",
    "start": "node dist/cli.js",
    "dev": "node --loader ts-node/esm src/cli.ts",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts --fix",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test && npm run lint",
    "test": "vitest",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "docs:generate": "typedoc"
  },
  "keywords": [
    "cli",
    "debugging",
    "ai",
    "logs",
    "extractor",
    "chrome",
    "devtools",
    "debug",
    "logging",
    "error-handling",
    "monitoring",
    "diagnostics",
    "troubleshooting",
    "log-analysis",
    "chrome-devtools-protocol",
    "development-tools",
    "testing"
  ],
  "author": "Jason Vaughan",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jasonvaughan/clits.git"
  },
  "bugs": {
    "url": "https://github.com/jasonvaughan/clits/issues"
  },
  "engines": {
    "node": ">=20"
  },
  "devDependencies": {
    "@playwright/test": "^1.52.0",
    "@types/chrome-remote-interface": "^0.31.7",
    "@types/inquirer": "^9.0.8",
    "@types/node": "^20.11.0",
    "@types/node-fetch": "^2.6.12",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "@vitest/coverage-v8": "^1.2.1",
    "@vitest/ui": "^1.2.1",
    "eslint": "^8.57.0",
    "rollup": "^4.12.0",
    "ts-node": "^10.9.2",
    "typedoc": "^0.28.5",
    "typedoc-plugin-markdown": "^4.6.4",
    "typescript": "^5.3.3",
    "vitest": "^1.6.1"
  },
  "dependencies": {
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.6",
    "@types/tabtab": "^3.0.4",
    "chalk": "^5.4.1",
    "chrome-remote-interface": "^0.33.3",
    "commander": "^12.0.0",
    "fast-glob": "^3.3.3",
    "glob": "^11.0.2",
    "inquirer": "^9.3.7",
    "node-fetch": "^3.3.2",
    "ora": "^8.2.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tabtab": "^3.0.2",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  }
}

{
  "name": "ai-debug-extractor",
  "version": "0.3.0",
  "description": "CLI tool and library for extracting and sharing debugging data for AI and web projects.",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./cli": {
      "import": "./dist/cli.js"
    }
  },
  "bin": {
    "ai-debug-extractor": "dist/cli.js",
    "ade": "dist/cli.js",
    "clits": "dist/cli.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "start": "node dist/cli.js",
    "dev": "ts-node src/cli.ts",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test && npm run lint",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage"
  },
  "keywords": [
    "cli",
    "debugging",
    "ai",
    "logs",
    "extractor",
    "chrome",
    "devtools",
    "debug",
    "logging",
    "error-handling",
    "monitoring",
    "diagnostics",
    "troubleshooting",
    "log-analysis",
    "chrome-devtools-protocol",
    "development-tools",
    "testing"
  ],
  "author": "Jason Vaughan",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jasonvaughan/ai-debug-extractor"
  },
  "bugs": {
    "url": "https://github.com/jasonvaughan/ai-debug-extractor/issues"
  },
  "engines": {
    "node": ">=16"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^20.11.0",
    "@types/node-fetch": "^2.6.12",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "chrome-remote-interface": "^0.33.3",
    "commander": "^12.0.0",
    "fast-glob": "^3.3.3",
    "glob": "^11.0.2",
    "node-fetch": "^3.3.2",
    "winston": "^3.17.0"
  }
}
